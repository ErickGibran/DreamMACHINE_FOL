float x = 400;
float y = 400;
float r = 70;
int levels = 3;

ArrayList<Circle> circles;


void setup() {
  size(800, 800);
  circles = new ArrayList<Circle>();
  
  
  for(int i = 0; levels > i; i++){
    for(int u = 0; u<6; u++){
  circles.add(new Circle(x, y, r));
  y -= r;}
  }
}

void draw(){
  background(0);
  
  
  for (Circle c : circles) {
  c.show();
  //c.grow();
}
  } 
